{
  "name": "balloon",
  "version": "2.0.0",
  "description": "Catch 'n plop all the balloons.",
  "main": "index.html",
  "env": {
    "browser": true
  },
  "scripts": {
    "prebuild": "npm run cleanclient",
    "build": "NODE_ENV=production webpack -p",
    "cleanclient": "del 'dist/*.js' 'dist/*.js.*' 'dist/*.html'",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "dev": "webpack-dev-server --inline",
    "fulltest": "parallelshell \"npm run lint:watch\" \"npm run test:watch\"",
    "lint": "eslint \"./src\"",
    "lint:watch": "watch \"npm run lint\" ./src",
    "prestart": "npm run build",
    "start": "http-server ./dist -o",
    "test": "jest",
    "test:debug": "node --debug-brk ./node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": ["src/**/*.{js,jsx}", "!**/node_modules/**"],
    "coverageReporters": ["json", "lcov", "text", "text-summary"]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.wetainment.com:57481/kriswep/balloon.git"
  },
  "keywords": [
    "game"
  ],
  "author": "kriswep",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-env": "^1.2.1",
    "css-loader": "^0.27.1",
    "del-cli": "^0.2.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.5.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "jest": "^19.0.2",
    "parallelshell": "^2.0.0",
    "regenerator-runtime": "^0.10.3",
    "style-loader": "^0.14.0",
    "supertest": "^3.0.0",
    "sw-precache": "^5.1.0",
    "url-loader": "^0.5.8",
    "watch": "^1.0.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {}
}
